             xmlns:c="clr-namespace:GlobalActions.GUI.Components"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GlobalActions.GUI.NodeSystem.Nodes.RepeatNode">

